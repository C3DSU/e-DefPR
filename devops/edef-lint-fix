source ${CLI_PATH}config/colors

cd $EDEF_PATH/backend

echo -e "${PURPLE}Fixing code indentation and spacing on backend...${NC}"
yarn standard --fix

cd $EDEF_PATH/frontend

echo -e "${PURPLE}Fixing code indentation and spacing on frontend...${NC}"
yarn standard --fix

echo -e "${GREEN}All done, code linted.${NC}"
